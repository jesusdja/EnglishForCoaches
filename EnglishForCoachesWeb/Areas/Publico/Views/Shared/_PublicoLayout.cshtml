@model EnglishForCoachesWeb.Areas.Publico.Models.ArticuloBaseViewModel

<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Basic Page Needs
    ================================================== -->
    <meta charset="utf-8">
    <!--[if IE]><meta http-equiv="x-ua-compatible" content="IE=9" /><![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>The Inim Co.</title>
    <meta name="description" content="English for Coaches is a Blog intended for coaches, players, referees and football professionals to study the football technical terminology in English necessary to perform their dutie.">
    <meta name="keywords" content="english, football, coaches, players, referee, terminology">
    <meta name="author" content="Real Federación Española de Fútbol">

    <meta property="og:site_name" content="English for Coaches - englishforcoaches.com" />
    <meta property="og:type" content="blog" />
    @RenderSection("head", required: false)

    <!-- Favicons
    ================================================== -->
    <link rel="shortcut icon" href="~/Content/Publico/img/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="~/Content/Publico/img/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/Content/Publico/img/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/Content/Publico/img/apple-touch-icon-114x114.png">
    <!-- Bootstrap
    ================================================== -->
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/bootstrap-tagsinput.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/jasny-bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/fonts/font-awesome-4.2.0/css/font-awesome.css">

    <!-- Animate
    ================================================== -->
    <link href='~/Content/Publico/css/effect2.css' rel='stylesheet' type='text/css'>
    <link href='~/Content/Publico/css/animate.css' rel='stylesheet' type='text/css'>

    <!-- Add fancyBox CSS files -->
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/js/fancybox/jquery.fancybox.css?v=2.1.5" media="screen" />

    <!-- Owl Slider CSS -->
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/owl.carousel.css" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/owl.theme.css" media="screen" />
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/owl.transitions.css" media="screen" />

    <!-- Custom Css
    ================================================== -->
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/rs-wp-v1.2.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/jquery.rs.selectbox.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/style.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/responsive.css">
    <!-- Custom Fonts
    ================================================== -->
    <link rel="stylesheet" type="text/css" href="~/Content/Publico/fonts/stylesheet.css">

    <!-- Google Fonts
    ================================================== -->
    <link href='http://fonts.googleapis.com/css?family=Lato:400,100,100italic,300,300italic,400italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>

    <link rel="stylesheet" type="text/css" href="~/Content/Publico/css/custom.css">
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <!--- Wrapper -->
    <section id="wrapper" class="clearfix">

        <!--- Header -->
        <header>
            <div class="rst-header-socialbar">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12">
                            @*<a href="http://rfef.es/" title="RFEF" target="_blank"><img class="rfef" src="~/Content/Publico/images/rfef.png" alt="RFEF" /></a>*@
                            <nav class="rst-socialbar-social">
                                <ul>
                                    <li>
                                        <form class="rst-search">
                                            <input type="text" placeholder="Search..." name="s" id="s">
                                            <button class="sb" id="btnBuscarLayout"><i class="fa fa-search"></i></button>
                                        </form>
                                    </li>
                                    <li>
                                        <div class="rst-loginform">
                                            <a href="http://englishforcoaches.com/" title="HOME">HOME</a>
                                        </div>
                                    </li>
                                </ul>
                            </nav>
                            <div class="clear"></div>
                        </div>
                    </div>
                </div>
            </div> <!-- end social bar -->
            <!-- Menu bar -->
            <div class="rst-header-menu">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="rst-header-menu-content">
                                <a class="rst-header-logo" href="http://englishforcoaches.com/" title="English for Coaches - englishforcaches.com"><img src="~/Content/Publico/images/header-logo.png?d=@DateTime.Now.Millisecond" alt="English for Coaches - englishforcaches.com" /></a>
                                <button class="rst-menu-trigger">
                                    <span>Toggle navigation</span>
                                </button>
                                <ul>
                                    <li><a href="@Url.Action("About")" title="About">About</a></li>
                                    @{
                                        Html.RenderPartial("~/Areas/Publico/Views/Shared/_Categorias.cshtml", Model.Categorias, new ViewDataDictionary());
                                    }

                                </ul>
                                <div class="clear"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <!--- End Header -->
        @RenderBody()
        <!--- Footer -->
        <footer>
            <div class="container">
                <div class="rst-footerline"></div>
                <div class="row rst-footer-content">
                    <div class="col-sm-3">
                        <span class="rst-menu-title">Menu</span>
                        <ul>
                            <li><a href="http://englishforcoaches.com" title="Home">Home</a></li>
                            <li><a href="@Url.Action("About")" title="About">About</a></li>
                            @{
                                Html.RenderPartial("~/Areas/Publico/Views/Shared/_Categorias.cshtml", Model.Categorias, new ViewDataDictionary());
                            }
                        </ul>
                    </div>
                    <div class="col-sm-6">
                        <span class="rst-menu-title">Tag cloud</span>
                        <ul class="cloud">
                            @{
                                Html.RenderPartial("~/Areas/Publico/Views/Shared/_NubeTags.cshtml", Model.TagsNube, new ViewDataDictionary());
                            }
                        </ul>
                    </div>
                    <div class="col-sm-3">
                        <span class="rst-menu-title">Contact</span>
                        <ul>
                            <li><strong>The Inim Co </strong></li>
                            @*<li><i class="fa fa-map-marker"></i> Calle Ramón y Cajal, s/n</li>
                                <li><i class="fa fa-map-marker"></i> 28230 Las Rozas, Madrid</li>*@
                            <li><i class="fa fa-envelope"></i> <a href="mailto:info@theinim.co" title="info@theinim.co">info@theinim.co</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="rst-footer-copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-xs-6 rst-copyright">
                            <p>© @DateTime.Now.Year The Inim Co.</p>
                        </div>
                        <div class="col-xs-6 rst-design">
                            <p>Powered by BDG Tech <a href="http://bdgtech.co/" title="bdgtech.co">bdgtech.co</a> <a href="#"><i class="fa fa-angle-up"></i></a></p>
                        </div>
                    </div>
                </div>
            </div>
        </footer><!--- End Footer -->

    </section><!--- End Wrapper -->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script type="text/javascript" src="~/Content/Publico/js/jquery.1.11.1.js"></script>

    <!-- Bootstrap Js Compiled Plugins -->
    <script type="text/javascript" src="~/Content/Publico/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/Publico/js/bootstrap-tagsinput.js"></script>
    <script type="text/javascript" src="~/Content/Publico/js/jqBootstrapValidation.js"></script>

    <!-- WoW Js -->
    <script type="text/javascript" src="~/Content/Publico/js/wow.min.js"></script>

    <!-- Add Fancybox -->
    <script type="text/javascript" src="~/Content/Publico/js/fancybox/jquery.fancybox.js?v=2.1.5"></script>
    <script type="text/javascript" src="~/Content/Publico/js/fancybox/helpers/jquery.fancybox-media.js?v=1.0.6"></script>

    <!-- Owl Slider Js -->
    <script type="text/javascript" src="~/Content/Publico/js/owl.carousel.js"></script>

    <!-- Weather Js -->
    <script type="text/javascript" src="~/Content/Publico/js/jquery.simpleWeather.js"></script>

    <!-- Custome Selectbox Js -->
    <script type="text/javascript" src="~/Content/Publico/js/jquery.rs.selectbox.js"></script>

    <script type="text/javascript" src="~/Content/Publico/js/main.js"></script>
    @RenderSection("scripts", required: false)


    <!-- Page JS Code -->
    <script>
            var abierto = false;
        function BuscarPublico()
            {
            console.log("entra");
            if (abierto) {
                var campoTexto = $('#s');

                if (campoTexto.val() == 'Search...')
                { campoTexto.val() = ''; }
                console.log('@(HttpContext.Current.Request.Url.GetLeftPart(UriPartial.Authority) + "/search/")' + campoTexto.val());
                location.href = '@(HttpContext.Current.Request.Url.GetLeftPart(UriPartial.Authority) + "/search/")' + campoTexto.val();
                abierto = false;
                return false;
            }else
            {
                abierto = true;
            }
        }

        $(function () {
            $('#btnBuscarLayout').on('click', BuscarPublico);
        });

    </script>

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-40212712-17', 'auto');
        ga('send', 'pageview');

    </script>
</body>
</html>
